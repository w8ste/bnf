<postal-address> ::= <name-part> <street-address> <zip-part>

<name-part> ::= <personal-part> <last-name> <opt-suffix-part> <EOL> | <personal-part> <name-part>

<name-part> ::= <test_double_defined>

<personal-part> ::= <first-name> | <initial> "."

<street-address> ::= <house-num> <street-name> <opt-apt-num> <EOL>

<zip-part> ::= <town-name> "," <state-code> <ZIP-code> <EOL>

<opt-suffix-part> ::= "Sr." | "Jr." | <roman-numeral> | ""

<opt-apt-num> ::= "Apt" <apt-num> | ""

<last-name> ::= "Smith" | "Johnson" | "Doe"

<first-name> ::= "Alice" | "Bob" | "Charlie"

<initial> ::= "A" | "B" | "C"

<house-num> ::= "123" | "456" | "789"

<street-name> ::= "Main St." | "Oak Ave." | "Maple Blvd."

<apt-num> ::= "1A" | "2B" | "3C"

<town-name> ::= "Springfield" | "Greenville" | "Centerville"

<state-code> ::= "NY" | "CA" | "TX"

<ZIP-code> ::= "12345" | "90210" | "77005"

<roman-numeral> ::= "I" | "II" | "III" | "IV"

<EOL> ::= "\n"
